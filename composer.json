{
    "name": "vigihdev/wp-cli-entity-command",
    "description": "A WP-CLI command for managing various WordPress entities (posts, pages, custom post types, taxonomies, terms, users, comments) efficiently via the command line.",
    "keywords": [
        "wp-cli",
        "wordpress-cli",
        "entity",
        "custom-post-type",
        "cpt",
        "taxonomy",
        "post-management",
        "wordpress-development",
        "command-line",
        "developer-tools",
        "content-management",
        "post-type",
        "term-management",
        "efficiency-tools"
    ],
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Vigih Dev",
            "email": "vigihdev@gmail.com",
            "role": "developer"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://satis.id"
        }
    ],
    "require": {
        "php": "^8.1",
        "vigihdev/wp-cli-models": "dev-main"
    },
    "autoload": {
        "psr-4": {
            "Vigihdev\\WpCliEntityCommand\\": "src/"
        },
        "files": [
            "entity-command.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Vigihdev\\WpCliEntityCommand\\Tests\\": "tests/"
        }
    },
    "config": {
        "process-timeout": 0
    },
    "extra": {
        "commands": [
            "post:list",
            "category:list",
            "menu:list"
        ]
    },
    "scripts": {
        "update:model": "console composer:update vigihdev/wp-cli-models",
        "build": "console git:push && console satis:build && cd $HOME/.wp-cli/packages && console composer:update vigihdev/wp-cli-entity-command && cd $HOME/WordpressCli/wp-cli-entity-command",
        "dev": "php -S localhost:2172 dev.php"
    }
}